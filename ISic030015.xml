                    <lb n="12"/><persName type="attested">Ͱιστι<gap reason="lost" extent="unknown" unit="character"/></persName>
                    <lb n="13"/><persName>Ἄ<unclear>π</unclear>ι<unclear>σ</unclear><gap reason="lost" extent="unknown" unit="character"/></persName>
                    <lb n="14"/><persName type="attested">Δαμ<gap reason="lost" extent="unknown" unit="character"/></persName>
