                    <lb n="8"/><persName type="attested"><gap reason="lost" extent="unknown" unit="character"/>μαρι<gap reason="lost" extent="unknown" unit="character"/></persName>
                    <lb n="9"/><persName type="attested">Μαιε<gap reason="lost" extent="unknown" unit="character"/></persName>
